Package: econetwork
Type: Package
Title: Analyzing Ecological Networks
Version: 0.7.0
Date: 2022-10-05
Authors@R: c(person("Vincent", "Miele", role=c("aut", "cre"), email="vincent.miele@univ-lyon1.fr"),
	   person("Catherine", "Matias", role=c("ctb")),
